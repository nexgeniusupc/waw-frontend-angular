{
  "name": "waw-frontend-angular",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --port 4200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "e2e": "ng e2e",
    "lint": "concurrently -s -n lint: 'npm:lint:*(!fix)'",
    "lint:eslint": "eslint --cache --ext .js,.ts,.html .",
    "lint:css": "stylelint --cache src/**/*.css",
    "lint:prettier": "prettier --check .",
    "lint:fix": "concurrently -s -n lint:fix: 'npm:lint:fix:*'",
    "lint:fix:eslint": "eslint --cache --ext .js,.ts,.html --fix .",
    "lint:fix:css": "stylelint --cache src/**/*.css --fix",
    "lint:fix:prettier": "prettier --write ."
  },
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "concurrently": "^7.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.2"
  }
}
